{
  "manifest_version": 3,
  "name": "LFM+",
  "short_name": "LFM+",
  "description": "UI/UX improvements for Low Fuel Motorsports",
  "version": "AUTO_FILLED_FROM_package.json",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png"
  },
  "action": {
    "default_title": "",
    "default_icon": "icons/icon128.png",
    "default_popup": "static/popup.html"
  },
  "background": {
    "service_worker": "worker.js"
  },
  "permissions": [
    "webNavigation",
    "webRequest",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://*.lowfuelmotorsport.com/"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/*"
      ],
      "matches": [
        "https://lowfuelmotorsport.com/*"
      ]
    }
  ]
}